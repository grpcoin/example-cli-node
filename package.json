{
  "name": "grpcoin",
  "description": "Grpc Coin Node cli",
  "version": "0.1.1",
  "author": "Kaan Karakaya @TheYkk",
  "bin": {
    "grpcoin": "./bin/run"
  },
  "bugs": "https://github.com/grpcoin/example-cli-node/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-plugins": "^1.10.0",
    "google-protobuf": "^3.17.1",
    "grpc": "^1.24.10",
    "grpc-tools": "^1.11.1",
    "grpc_tools_node_protoc_ts": "^5.2.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/google-protobuf": "^3.15.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "homepage": "https://github.com/grpcoin/example-cli-node",
  "keywords": [
    "oclif"
  ],
  "license": "Apache 2.0",
  "main": "dist/index.js",
  "oclif": {
    "bin": "grpcoin",
    "commands": "./dist/commands",
    "helpClass": "./dist/lib/help",
    "plugins": [
      "@oclif/plugin-not-found",
      "@oclif/plugin-help"
    ]
  },
  "repository": "TheYkk/grpcoin",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "fix": "eslint . --ext .ts --config .eslintrc --fix",
    "prepack": "rm -rf dist && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
